                    <p class="mx-lg-5">
                       @item.Description
                    </p>
                </div><div class="row">
                    <div class="col-lg-8 mx-lg-auto col-md-10">
                        <img src="@item.Image" alt="" class="img-fluid mt-lg-5 mt-3">
